#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/project/nuxt-monorepo/node_modules/.pnpm/nuxt@3.13.2_@parcel+watcher@2.4.1_@types+node@22.8.4_eslint@9.13.0_jiti@2.3.3__ioredis@5.4.1__dycd24hdhe6xsy3tlcfnfsy7gu/node_modules/nuxt/bin/node_modules:/Users/project/nuxt-monorepo/node_modules/.pnpm/nuxt@3.13.2_@parcel+watcher@2.4.1_@types+node@22.8.4_eslint@9.13.0_jiti@2.3.3__ioredis@5.4.1__dycd24hdhe6xsy3tlcfnfsy7gu/node_modules/nuxt/node_modules:/Users/project/nuxt-monorepo/node_modules/.pnpm/nuxt@3.13.2_@parcel+watcher@2.4.1_@types+node@22.8.4_eslint@9.13.0_jiti@2.3.3__ioredis@5.4.1__dycd24hdhe6xsy3tlcfnfsy7gu/node_modules:/Users/project/nuxt-monorepo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/project/nuxt-monorepo/node_modules/.pnpm/nuxt@3.13.2_@parcel+watcher@2.4.1_@types+node@22.8.4_eslint@9.13.0_jiti@2.3.3__ioredis@5.4.1__dycd24hdhe6xsy3tlcfnfsy7gu/node_modules/nuxt/bin/node_modules:/Users/project/nuxt-monorepo/node_modules/.pnpm/nuxt@3.13.2_@parcel+watcher@2.4.1_@types+node@22.8.4_eslint@9.13.0_jiti@2.3.3__ioredis@5.4.1__dycd24hdhe6xsy3tlcfnfsy7gu/node_modules/nuxt/node_modules:/Users/project/nuxt-monorepo/node_modules/.pnpm/nuxt@3.13.2_@parcel+watcher@2.4.1_@types+node@22.8.4_eslint@9.13.0_jiti@2.3.3__ioredis@5.4.1__dycd24hdhe6xsy3tlcfnfsy7gu/node_modules:/Users/project/nuxt-monorepo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../nuxt/bin/nuxt.mjs" "$@"
else
  exec node  "$basedir/../nuxt/bin/nuxt.mjs" "$@"
fi
